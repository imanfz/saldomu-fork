<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/llMemberDetail"
    tools:context="com.sgo.saldomu.fragments.FragSetttingKelola">


    <TextView
        android:id="@+id/tvMemberNameLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:height="30dp"

        android:gravity="left"
        android:text="@string/agent_name"
        android:textSize="@dimen/abc_text_size_small_material"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/tvDetailMemberName"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvDetailMemberName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:height="30dp"
        android:gravity="right"
        tools:text="tgbtbtgbtgb"
        android:textSize="@dimen/abc_text_size_small_material"
        app:layout_constraintLeft_toRightOf="@+id/tvMemberNameLabel"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvMemberNameLabel" />


    <TextView
        android:id="@+id/tvCategoryNameLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:height="30dp"

        android:gravity="left"
        android:text="@string/category"
        android:textSize="@dimen/abc_text_size_small_material"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvMemberNameLabel" />

    <TextView
        android:id="@+id/tvCategoryName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:height="30dp"
        android:gravity="right"
        tools:text="tgbtbtgbtgb"
        android:textSize="@dimen/abc_text_size_small_material"
        app:layout_constraintLeft_toRightOf="@+id/tvCategoryNameLabel"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvCategoryNameLabel" />


    <TextView
        android:id="@+id/tvCommNameLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:height="30dp"

        android:gravity="left"
        android:text="@string/shop_name"
        android:textSize="@dimen/abc_text_size_small_material"
        app:layout_constraintHorizontal_bias="0.17"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/tvCommName"
        app:layout_constraintTop_toBottomOf="@+id/tvCategoryNameLabel" />

    <TextView
        android:id="@+id/tvCommName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:height="30dp"
        android:gravity="right"
        tools:text="tgbtbtgbtgb"
        android:textSize="@dimen/abc_text_size_small_material"
        app:layout_constraintLeft_toRightOf="@+id/tvCommNameLabel"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvCommNameLabel" />


    <TextView
        android:id="@+id/tvAddressLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:height="30dp"
        android:gravity="left"

        android:text="@string/myprofile_text_address"
        android:textSize="@dimen/abc_text_size_small_material"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvCommNameLabel"
        app:layout_constraintRight_toLeftOf="@+id/tvAddress"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.07"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/tvAddress"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:height="30dp"
        android:gravity="right"
        tools:text="tgbtbtgbtgb"
        android:textSize="@dimen/abc_text_size_small_material"
        app:layout_constraintLeft_toRightOf="@+id/tvAddressLabel"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvAddressLabel"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="16dp" />


    <TextView
        android:id="@+id/tvTutupSekarangLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:height="30dp"
        android:gravity="left"
        android:text="@string/set_online"
        android:textSize="@dimen/abc_text_size_small_material"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tvAddressLabel" />

    <Switch
        android:id="@+id/swTutupToko"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/tvTutupSekarangLabel"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="3dp"
        android:gravity="right"
        android:textOff="@string/no"
        android:textOn="@string/yes"
        app:layout_constraintHorizontal_bias="0.527"
        app:layout_constraintLeft_toRightOf="@+id/tvTutupSekarangLabel"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvTutupSekarangLabel" />


    <Button
            android:id="@+id/btnSettingLokasi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/tvAddressLabel"
            android:text="@string/update_merchant_location"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lvSetting" />

    <ListView
        android:id="@+id/lvSetting"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_below="@+id/tvTutupSekarangLabel"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:divider="@null"
        android:dividerHeight="0dp"
        android:footerDividersEnabled="false"
        app:layout_constraintBottom_toTopOf="@+id/btnSettingLokasi"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/swTutupToko">

    </ListView>


</android.support.constraint.ConstraintLayout>
