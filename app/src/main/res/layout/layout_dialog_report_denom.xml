<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_marginLeft="@dimen/padding_small"
    android:layout_marginTop="@dimen/abc_text_size_medium_material"
    android:visibility="visible"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:stretchColumns="1">
    <TableRow>
        <TextView
            android:text="@string/report_transaction_type"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="3dip"
            />
        <TextView
            android:id="@+id/tv_report_transaction_type"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:gravity="end"
            android:padding="3dip" />
    </TableRow>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_margin="8dp"
        android:background="@color/grey_300"/>

    <TableRow>

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="3dip"
            android:text="@string/store_code" />
        <TextView
            android:id="@+id/dialog_denom_store_code"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:gravity="end"
            android:padding="3dip" />
    </TableRow>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_margin="8dp"
        android:background="@color/grey_300"/>

    <TableRow>
        <TextView
            android:text="@string/store_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="3dip"
            />
        <TextView
            android:id="@+id/tv_report_store_name"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:gravity="end"
            android:padding="3dip" />
    </TableRow>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_margin="8dp"
        android:background="@color/grey_300"/>

    <TableRow>
        <TextView
            android:text="@string/store_address"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="3dip"
            />
        <TextView
            android:id="@+id/tv_report_store_address"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:gravity="end"
            android:padding="3dip" />
    </TableRow>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_margin="8dp"
        android:background="@color/grey_300"/>

    <TableRow>
        <TextView
                android:text="@string/community_code"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="3dip"
                />
        <TextView
                android:id="@+id/dialog_denom_comm_code"
                android:layout_weight="2"
                android:layout_width="0dp"
                android:gravity="end"
                android:padding="3dip" />
    </TableRow>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_margin="8dp"
        android:background="@color/grey_300"/>

    <TableRow>
        <TextView
            android:text="@string/agent_name"
            android:layout_span="2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="3dip"
            />
        <TextView
            android:id="@+id/dialog_denom_agent_name"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:gravity="end"
            android:padding="3dip" />
    </TableRow>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_margin="8dp"
        android:background="@color/grey_300"/>

    <TableRow>
        <TextView
            android:text="@string/agent_phone"
            android:layout_span="2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="3dip"
            />
        <TextView
            android:id="@+id/dialog_denom_agent_phone"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:gravity="end"
            android:padding="3dip" />
    </TableRow>

    <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_margin="8dp"
            android:background="@color/grey_300"/>

    <TableRow>
        <TextView
            android:text="@string/order_number"
            android:layout_span="2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="3dip"
            />
        <TextView
            android:id="@+id/dialog_denom_order_number"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:gravity="end"
            android:padding="3dip" />
    </TableRow>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_margin="8dp"
        android:background="@color/grey_300"/>

    <TableRow>
        <TextView
                android:text="@string/denom_item"
                android:layout_span="2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="3dip" />

    </TableRow>

    <!--<TableRow-->
        <!--android:gravity="center">-->
        <!--<android.support.v7.widget.RecyclerView-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:id="@+id/dialog_denom_item_list_recyclerview"-->
            <!--android:visibility="gone">-->

        <!--</android.support.v7.widget.RecyclerView>-->

    <!--</TableRow>-->
    <TableLayout android:id="@+id/billertoken_layout_table"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:layout_height="wrap_content"
        android:layout_marginRight="4dp"
        android:stretchColumns="1">


    </TableLayout>

    <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_margin="8dp"
            android:background="@color/grey_300"/>

    <TableRow>
        <TextView
                android:text="@string/bank_product"
                android:layout_span="2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="3dip" />
        <TextView
                android:id="@+id/dialog_denom_bank_product"
                android:layout_gravity="center_vertical"
                android:layout_weight="2"
                android:layout_width="0dp"
                android:gravity="end"
                android:padding="3dip" />
    </TableRow>

    <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_margin="8dp"
            android:background="@color/grey_300"/>

    <TableRow>
        <TextView
            android:text="@string/jumlah"
            android:layout_span="2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="3dip" />
        <TextView
            android:id="@+id/dialog_denom_amount"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:gravity="end"
            android:padding="3dip"
            android:maxLines="1" />
    </TableRow>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_margin="8dp"
        android:background="@color/grey_300"/>

    <TableRow>
        <TextView
            android:text="@string/cashout_fee_text"
            android:layout_span="2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="3dip" />
        <TextView
            android:id="@+id/dialog_denom_fee_value"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:gravity="end"
            android:padding="3dip"
            android:maxLines="1" />
    </TableRow>

    <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_margin="8dp"
            android:background="@color/grey_300"/>

    <TableRow>
        <TextView
            android:text="@string/total_amount"
            android:layout_span="2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="3dip" />
        <TextView
            android:id="@+id/dialog_denom_totalamount_value"
            android:layout_weight="2"
            android:layout_width="0dp"
            android:gravity="end"
            android:padding="3dip"
            android:maxLines="1" />
    </TableRow>

</TableLayout>